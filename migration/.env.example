# Example environment configuration for Access to PostgreSQL migration

# PostgreSQL Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=migrated_db
DB_USER=postgres
DB_PASSWORD=your_password_here

# Access Database Configuration
ACCESS_DB_PATH=database.mdb

# Migration Settings
BATCH_SIZE=1000
CREATE_INDEXES=true
PRESERVE_RELATIONSHIPS=true
CONVERT_AUTONUMBER_TO_SERIAL=true

# Logging
LOG_LEVEL=INFO